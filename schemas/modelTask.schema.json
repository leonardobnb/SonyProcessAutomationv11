{
  "type": "object",
  "id": "#/Task",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1,
      "maximum": 32767
    },
    "name": {
      "type": "string",
      "pattern": "^[A-Z][A-Za-z0-9]*"
    },
    "routePath": {
      "type": "string"
    },
    "businessObject": {
      "type": "string"
    },
    "securityDomains": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "secured": {
      "type": "boolean"
    },
    "label": {
      "type": "string",
      "pattern": "^[A-Z|0-9][A-Za-z0-9:\\-\\s]*"
    }
  },
  "required": [
    "id",
    "name",
    "routePath",
    "securityDomains"
  ],
  "additionalProperties": false
}
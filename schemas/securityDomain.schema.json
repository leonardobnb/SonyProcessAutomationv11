      "type": "integer",
      "minimum": 1,
      "maximum": 32767
    },
    "name": {
      "type": "string",
      "pattern": "^[A-Z][A-Za-z0-9]*"
    },
    "label": {
      "type": "string",
      "pattern": "^[A-Z|0-9][A-Za-z0-9:\\-\\s]*"
    },
    "enabledForPageSecurity": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "name"
  ],
  "additionalProperties": false
}